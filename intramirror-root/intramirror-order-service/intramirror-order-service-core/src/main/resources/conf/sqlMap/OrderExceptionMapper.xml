<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.intramirror.order.core.mapper.OrderExceptionMapper">
  
   
   <insert id="saveOrderComments" parameterType="java.util.Map">
   INSERT INTO order_exception(logistics_product_id,comments,order_exception_type_id,`status`,created_at)
	VALUE(#{logistics_product_id,jdbcType=BIGINT},#{comments,jdbcType=VARCHAR},#{order_exception_type_id,jdbcType=VARCHAR},#{status,jdbcType=BIGINT},#{created_at,jdbcType=TIMESTAMP})
   </insert>
</mapper>