<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.intramirror.product.core.mapper.TagProductRelMapper">

    <insert id="saveTagProductRel" parameterType="java.util.Map">

        INSERT INTO tag_product_rel(
        tag_id,
        product_id,
        created_at)
        VALUES
        <foreach collection="productIdList" item="item" index="index" separator=",">
            (#{tagId,jdbcType=BIGINT},#{item,jdbcType=BIGINT},#{created_at,jdbcType=TIMESTAMP})
        </foreach>
    </insert>
</mapper>